{
  "inbounds": [
    {
      "port": 8080,
      "listen":"0.0.0.0",
      "protocol": "vmess",
      "settings": {
        "clients": [
          {{ clients }}
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
        "path": "/{{ path }}"
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}