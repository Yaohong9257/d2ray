{
  "inbounds": [
    {
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {{ clients }}
        ],
        "decryption": "none",
        "fallbacks": [
          {
            "dest": "xray_nginx:80"
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "xtls",
        "xtlsSettings": {
          "certificates:": [
            {
              "certificateFile": "/certs/fullchain.pem",
              "keyFile": "/certs/privkey.pem"
            }
          ]
        }
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "settings": {}
    }
  ]
}